    "mongoose": "^4.12.6",
    "node-fetch": "^1.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-apollo": "^1.4.16",
    "react-dom": "^16.0.0",
    "react-phone-number-input": "^0.15.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rollbar": "^2.2.7",
    "subscriptions-transport-ws": "^0.8.2",
    "twilio": "^3.6.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "eslint": "4.13.1",
    "eslint-config-airbnb": "^16.1.0",
