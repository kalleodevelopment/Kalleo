{
  "name": "kalleo-app-server",
  "description": "GraphQL server for Kalleo app",
  "version": "0.0.0",
  "author": "Poetic Systems",
  "contributors": [
    "Evan Corl <evan@poeticsystems.com>",
    "Chuong Le <chuong@poeticsystems.com",
    "Kristy Miller <kristy@poeticsystems.com"
  ],
  "private": true,
  "scripts": {
    "community:start": "cd ../kalleo-community-api && yarn run dev",
    "dev": "meteor run --settings settings-dev.json",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "native:start": "cd ../kalleo-native && npm start",
    "server:start": "npm run dev",
    "start": "concurrently -n \"SERVER,MONGO,NATIVE,COMMUNITY\" -c \"yellow,blue,green,cyan\" \"npm run server:start\" \"mongod\" \"npm run native:start\" \"npm run community:start\""
  },
  "dependencies": {
