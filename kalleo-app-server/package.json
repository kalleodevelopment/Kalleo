    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-runtime": "^6.20.0",
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.1",
    "decko": "^1.2.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "graphql": "^0.10.5",
    "graphql-errors": "^2.1.0",
    "graphql-redis-subscriptions": "^1.3.1",
    "graphql-server-express": "^1.0.4",
    "graphql-subscriptions": "^0.4.4",
    "graphql-tag": "^2.4.2",
    "graphql-tools": "^1.1.0",
    "helmet": "^3.12.0",
    "ioredis": "^3.1.4",
    "jwt-mongo-sms": "^2.4.2",
    "jwt-simple": "^0.5.1",
    "keymirror": "^0.1.1",
